<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000">

    <RelativeLayout
        android:layout_width="300dp"
        android:layout_height="360dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/bg_dashang">

        <LinearLayout
            android:id="@+id/dashang_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="25dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="您剩余团币:"
                android:textColor="#777777"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/dashang_mycount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="111"
                android:textColor="#777777"
                android:textSize="13sp" />
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/dashang_exit"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_alignParentRight="true">
        <ImageView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:src="@mipmap/dashang_exit" />
        </RelativeLayout>
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="56dp"
            android:src="@mipmap/dashang_moneyline" />

        <RelativeLayout
            android:id="@+id/auto_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="91dp">


            <RelativeLayout
                android:id="@+id/two_money"
                android:layout_width="72.5dp"
                android:layout_height="34dp"
                android:background="@drawable/bg_dashang_money"
                android:clickable="true">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="2"
                        android:textColor="#d5aa5f"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/tv_two_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:text="币"
                        android:textColor="#d5aa5f"
                        android:textSize="11sp" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/five_money"
                android:layout_width="72.5dp"
                android:layout_height="34dp"
                android:layout_marginLeft="14dp"
                android:layout_toRightOf="@+id/two_money"
                android:background="@drawable/bg_dashang_money"
                android:clickable="true">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_five"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="5"
                        android:textColor="#d5aa5f"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/tv_five_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:text="币"
                        android:textColor="#d5aa5f"
                        android:textSize="11sp" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ten_money"
                android:layout_width="72.5dp"
                android:layout_height="34dp"
                android:layout_marginLeft="14dp"
                android:layout_toRightOf="@+id/five_money"
                android:background="@drawable/bg_dashang_money"
                android:clickable="true">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_ten"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="10"
                        android:textColor="#d5aa5f"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/tv_ten_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:text="币"
                        android:textColor="#d5aa5f"
                        android:textSize="11sp" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/twenty_money"
                android:layout_width="72.5dp"
                android:layout_height="34dp"
                android:layout_below="@+id/two_money"
                android:layout_marginTop="16dp"
                android:background="@drawable/bg_dashang_money"
                android:clickable="true">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_twenty"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="20"
                        android:textColor="#d5aa5f"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/tv_twenty_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:text="币"
                        android:textColor="#d5aa5f"
                        android:textSize="11sp" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/fifty_money"
                android:layout_width="72.5dp"
                android:layout_height="34dp"
                android:layout_below="@+id/two_money"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="16dp"
                android:layout_toRightOf="@+id/twenty_money"
                android:background="@drawable/bg_dashang_money"
                android:clickable="true">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_fifty"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="50"
                        android:textColor="#d5aa5f"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/tv_fifty_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:text="币"
                        android:textColor="#d5aa5f"
                        android:textSize="11sp" />
                </LinearLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/hundred_money"
                android:layout_width="72.5dp"
                android:layout_height="34dp"
                android:layout_below="@+id/two_money"
                android:layout_marginLeft="14dp"
                android:layout_marginTop="16dp"
                android:layout_toRightOf="@+id/fifty_money"
                android:background="@drawable/bg_dashang_money"
                android:clickable="true">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_hundred"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="100"
                        android:textColor="#d5aa5f"
                        android:textSize="16sp" />

                    <TextView
                        android:id="@+id/tv_hundred_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="2dp"
                        android:text="币"
                        android:textColor="#d5aa5f"
                        android:textSize="11sp" />
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>

        <ImageView
            android:id="@+id/dashang_line"
            android:layout_width="220dp"
            android:layout_height="1dp"
            android:layout_below="@id/auto_money"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="65dp"
            android:background="#d5aa5f" />

        <ImageView
            android:layout_width="11dp"
            android:layout_height="12.5dp"
            android:layout_below="@id/auto_money"
            android:layout_marginLeft="41dp"
            android:layout_marginTop="40dp"
            android:src="@mipmap/dashang_money_icon" />

        <EditText
            android:id="@+id/et_addmoney"
            android:layout_width="160dp"
            android:layout_height="30dp"
            android:layout_below="@id/auto_money"
            android:layout_marginLeft="73dp"
            android:layout_marginTop="32dp"
            android:background="@null"
            android:enabled="true"
            android:hint="手动输入团币"
            android:inputType="number"
            android:textColor="#d5aa5f"
            android:textColorHint="#80d5aa5f"
            android:textSize="18sp" />

        <ImageView
            android:id="@+id/im_deletemoney"
            android:layout_width="15dp"
            android:layout_height="15dp"
            android:layout_alignParentRight="true"
            android:layout_below="@id/auto_money"
            android:layout_marginRight="41dp"
            android:layout_marginTop="40dp"
            android:src="@mipmap/dashang_deletemoney"
            android:visibility="gone" />

        <TextView
            android:id="@+id/tv_dashang_true"
            android:layout_width="244dp"
            android:layout_height="34dp"
            android:layout_alignLeft="@+id/auto_money"
            android:layout_alignStart="@+id/auto_money"
            android:layout_below="@+id/dashang_line"
            android:layout_marginTop="36dp"
            android:background="@drawable/bg_dashang_true"
            android:gravity="center"
            android:text="打赏"
            android:textColor="#ffffff"
            android:textSize="15dp" />

        <LinearLayout
            android:id="@+id/dashang_reminder"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/tv_dashang_true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:text="打赏记录"
                android:textColor="#959595"
                android:textSize="10dp" />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:src="@mipmap/dashang_next" />

        </LinearLayout>


    </RelativeLayout>


</RelativeLayout>