<?xml version="1.0" encoding="utf-8"?><!-- 加入组dialog界面 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/tv_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_marginTop="25dp"
                    android:text="选择你使用的地图"
                    android:textColor="@color/bg_profit_gray"
                    android:textSize="@dimen/fourteen" />
            </RelativeLayout>

            <Button
                android:id="@+id/btn_baidu_map"
                android:layout_width="250dp"
                android:layout_height="35dp"
                android:layout_marginLeft="@dimen/margin_twenty_five"
                android:layout_marginRight="@dimen/margin_twenty_five"
                android:layout_marginTop="@dimen/margin_twenty_five"
                android:background="@drawable/btn_orange_shpae"
                android:text="百度地图"
                android:textColor="@color/bg_button_text_black"
                android:textSize="@dimen/fifteen"
                android:visibility="gone" />

            <Button
                android:id="@+id/btn_gaode_map"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginLeft="@dimen/margin_twenty_five"
                android:layout_marginRight="@dimen/margin_twenty_five"
                android:layout_marginTop="@dimen/margin_twenty"
                android:background="@drawable/btn_orange_shpae"
                android:text="高德地图"
                android:textColor="@color/bg_button_text_black"
                android:textSize="@dimen/fifteen"
                android:visibility="gone" />

            <Button
                android:id="@+id/btn_tencent_map"
                android:layout_width="match_parent"
                android:layout_height="35dp"
                android:layout_marginLeft="@dimen/margin_twenty_five"
                android:layout_marginRight="@dimen/margin_twenty_five"
                android:layout_marginTop="@dimen/margin_twenty"
                android:background="@drawable/btn_orange_shpae"
                android:text="腾讯地图"
                android:textColor="@color/bg_button_text_black"
                android:textSize="@dimen/fifteen"
                android:visibility="gone" />

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="@dimen/margin_twenty"
                android:background="@color/bg_line" />

            <TextView
                android:id="@+id/tv_cancle"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginLeft="@dimen/margin_ten"
                android:layout_marginRight="@dimen/margin_ten"
                android:gravity="center"
                android:text="取消"
                android:textColor="@color/bg_new_guide_black"
                android:textSize="@dimen/fifteen" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>
