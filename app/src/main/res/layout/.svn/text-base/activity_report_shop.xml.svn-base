<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backround_7">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:background="@color/bg_white">

            <RelativeLayout
                android:id="@+id/rl_back"
                android:layout_width="45dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="10dp"
                    android:background="@mipmap/rl_back" />
            </RelativeLayout>

            <TextView
                android:id="@+id/tv_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="输入预约信息"
                android:textColor="@color/bg_text_black"
                android:textSize="@dimen/sixteen" />

            <RelativeLayout
                android:id="@+id/rl_publish"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:visibility="gone">

                <TextView
                    android:id="@+id/tv_right"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text=""
                    android:textColor="@color/bg_text_black"
                    android:textSize="@dimen/sixteen" />
            </RelativeLayout>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/bg_line" />

        <ScrollView
            android:id="@+id/scroll_view"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:scrollbars="none">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="店名:"
                            android:textColor="@color/baobei_black_text"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="45dp"
                        android:layout_weight="4">

                        <TextView
                            android:id="@+id/tv_shop_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:text=""
                            android:textColor="@color/baobei_black_text"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/bg_white"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="旅行社:"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4">

                        <EditText
                            android:id="@+id/et_travel_name"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_above="@+id/view_1"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:background="@color/bg_white"
                            android:gravity="center_vertical"
                            android:hint="请输入旅行社名称"
                            android:inputType="text"
                            android:maxLength="15"
                            android:textColor="@color/baobei_black_text"
                            android:textSize="@dimen/thirteen" />

                        <View
                            android:id="@+id/view_1"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_marginLeft="@dimen/margin_ten"
                            android:background="@color/bg_line" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/bg_white"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="导游:"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4">

                        <EditText
                            android:id="@+id/tv_name"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_above="@+id/view_2"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:background="@color/bg_white"
                            android:gravity="center_vertical"
                            android:hint="请输入姓名"
                            android:inputType="text"
                            android:maxLength="12"
                            android:textColor="@color/baobei_black_text"
                            android:textSize="@dimen/thirteen" />

                        <View
                            android:id="@+id/view_2"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_marginLeft="@dimen/margin_ten"
                            android:background="@color/bg_line" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/bg_white"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="手机:"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4">

                        <EditText
                            android:id="@+id/tv_phone"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_above="@+id/view_3"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:background="@color/bg_white"
                            android:gravity="center_vertical"
                            android:hint="请输入手机号码"
                            android:inputType="phone"
                            android:maxLength="11"
                            android:textColor="@color/baobei_black_text"
                            android:textSize="@dimen/thirteen" />

                        <View
                            android:id="@+id/view_3"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_marginLeft="@dimen/margin_ten"
                            android:background="@color/bg_line" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/bg_white"
                    android:orientation="horizontal">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="车牌:"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="4">

                        <EditText
                            android:id="@+id/tv_car_num"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_above="@+id/view_4"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:background="@color/bg_white"
                            android:gravity="center_vertical"
                            android:hint="请输入本次旅行车牌号"
                            android:inputType="text"
                            android:maxLength="10"
                            android:singleLine="true"
                            android:textColor="@color/baobei_black_text"
                            android:textSize="@dimen/thirteen" />

                        <View
                            android:id="@+id/view_4"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_alignParentBottom="true"
                            android:layout_marginLeft="@dimen/margin_ten"
                            android:background="@color/bg_line" />
                    </RelativeLayout>
                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"
                                android:text="人数:"
                                android:textColor="@color/bg_push_time"
                                android:textSize="@dimen/thirteen" />
                        </RelativeLayout>

                        <RelativeLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="4">
                            <!-- 定义一组单选按钮 -->
                            <RadioGroup
                                android:id="@+id/rp_num"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <RadioButton
                                    android:id="@+id/radio_num"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="@dimen/margin_fifteen"
                                    android:layout_weight="1"
                                    android:button="@drawable/baobei_checkbox_selected"
                                    android:checked="true"
                                    android:text="  按数量"
                                    android:textColor="@color/baobei_black_text"
                                    android:textSize="@dimen/thirteen" />

                                <RadioButton
                                    android:id="@+id/radio_size"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical"
                                    android:layout_weight="1"
                                    android:button="@drawable/baobei_checkbox_selected"
                                    android:text="  按大小"
                                    android:textColor="@color/baobei_black_text"
                                    android:textSize="@dimen/thirteen" />

                                <RadioButton
                                    android:id="@+id/radio_female"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_vertical"
                                    android:layout_weight="1"
                                    android:button="@drawable/baobei_checkbox_selected"
                                    android:text="  按性别"
                                    android:textColor="@color/baobei_black_text"
                                    android:textSize="@dimen/thirteen" />
                            </RadioGroup>
                        </RelativeLayout>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/img_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:background="@mipmap/baobei_bg_num" />

                    <ImageView
                        android:id="@+id/img_size"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:background="@mipmap/baobei_bg_size"
                        android:visibility="gone" />

                    <ImageView
                        android:id="@+id/img_sex"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:background="@mipmap/baobei_bg_sex"
                        android:visibility="gone" />
                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/ll_num"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/bg_white">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="数量:"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5">

                        <EditText
                            android:id="@+id/et_by_num"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:background="@color/bg_white"
                            android:gravity="center_vertical"
                            android:hint="请输入人数"
                            android:inputType="number"
                            android:maxLength="5"
                            android:textColor="@color/baobei_black_text"
                            android:textColorHint="@color/bg_goods_search_text"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2.5">

                        <View
                            android:id="@+id/view_5"
                            android:layout_width="1dp"
                            android:layout_height="15dp"
                            android:layout_centerVertical="true"
                            android:background="@color/bg_main_statue" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_ten"
                            android:layout_toRightOf="@+id/view_5"
                            android:text="人"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:id="@+id/view_num"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/bg_line" />

                <LinearLayout
                    android:id="@+id/ll_adult"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/bg_white"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="大人:"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5">

                        <EditText
                            android:id="@+id/et_by_adult"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:background="@color/bg_white"
                            android:gravity="center_vertical"
                            android:hint="请输入人数"
                            android:inputType="number"
                            android:maxLength="5"
                            android:textColor="@color/baobei_black_text"
                            android:textColorHint="@color/bg_goods_search_text"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2.5">

                        <View
                            android:id="@+id/view_6"
                            android:layout_width="1dp"
                            android:layout_height="15dp"
                            android:layout_centerVertical="true"
                            android:background="@color/bg_main_statue" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_ten"
                            android:layout_toRightOf="@+id/view_6"
                            android:text="人"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:id="@+id/view_adult"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/bg_line" />

                <LinearLayout
                    android:id="@+id/ll_child"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/bg_white"
                    android:orientation="horizontal"
                    android:visibility="gone">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="孩童:"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5">

                        <EditText
                            android:id="@+id/et_by_child"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:background="@color/bg_white"
                            android:gravity="center_vertical"
                            android:hint="请输入人数"
                            android:inputType="number"
                            android:maxLength="5"
                            android:textColor="@color/baobei_black_text"
                            android:textColorHint="@color/bg_goods_search_text"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2.5">

                        <View
                            android:id="@+id/view_7"
                            android:layout_width="1dp"
                            android:layout_height="15dp"
                            android:layout_centerVertical="true"
                            android:background="@color/bg_main_statue" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_ten"
                            android:layout_toRightOf="@+id/view_7"
                            android:text="人"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/ll_male"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/bg_white"
                    android:visibility="gone">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="男:"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5">

                        <EditText
                            android:id="@+id/et_by_boy"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:background="@color/bg_white"
                            android:gravity="center_vertical"
                            android:hint="请输入人数"
                            android:inputType="number"
                            android:maxLength="5"
                            android:textColor="@color/baobei_black_text"
                            android:textColorHint="@color/bg_goods_search_text"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2.5">

                        <View
                            android:id="@+id/view_8"
                            android:layout_width="1dp"
                            android:layout_height="15dp"
                            android:layout_centerVertical="true"
                            android:background="@color/bg_main_statue" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_ten"
                            android:layout_toRightOf="@+id/view_8"
                            android:text="人"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:id="@+id/view_boy"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/bg_line" />

                <LinearLayout
                    android:id="@+id/ll_femal"
                    android:layout_width="match_parent"
                    android:layout_height="45dp"
                    android:background="@color/bg_white"
                    android:visibility="gone">

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"
                            android:text="女:"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="1.5">

                        <EditText
                            android:id="@+id/et_by_girl"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_marginLeft="@dimen/margin_fifteen"
                            android:background="@color/bg_white"
                            android:gravity="center_vertical"
                            android:hint="请输入人数"
                            android:inputType="number"
                            android:maxLength="5"
                            android:textColor="@color/baobei_black_text"
                            android:textColorHint="@color/bg_goods_search_text"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="2.5">

                        <View
                            android:id="@+id/view_9"
                            android:layout_width="1dp"
                            android:layout_height="15dp"
                            android:layout_centerVertical="true"
                            android:background="@color/bg_main_statue" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginLeft="@dimen/margin_ten"
                            android:layout_toRightOf="@+id/view_9"
                            android:text="人"
                            android:textColor="@color/bg_bottom_gray"
                            android:textSize="@dimen/thirteen" />
                    </RelativeLayout>
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
