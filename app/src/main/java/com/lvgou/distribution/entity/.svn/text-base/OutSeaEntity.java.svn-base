package com.lvgou.distribution.entity;

/**
 * Created by Snow on 2016/3/29 0029.
 */
public class OutSeaEntity {
    private String id;
    private String img_path;
    private String name;
    private String price;// 原价
    private String discount_price;// 售价
    private String commission;// 佣金 比列 = commission*discount_price

    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getImg_path() {
        return img_path;
    }

    public void setImg_path(String img_path) {
        this.img_path = img_path;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getPrice() {
        return price;
    }

    public void setPrice(String price) {
        this.price = price;
    }

    public String getDiscount_price() {
        return discount_price;
    }

    public void setDiscount_price(String discount_price) {
        this.discount_price = discount_price;
    }

    public String getCommission() {
        return commission;
    }

    public void setCommission(String commission) {
        this.commission = commission;
    }

    public OutSeaEntity(String id, String img_path, String price, String name, String discount_price, String commission) {
        this.id = id;
        this.img_path = img_path;
        this.price = price;
        this.name = name;
        this.discount_price = discount_price;
        this.commission = commission;
    }

    public OutSeaEntity() {
    }

    @Override
    public String toString() {
        return "OutSeaEntity{" +
                "id='" + id + '\'' +
                ", img_path='" + img_path + '\'' +
                ", name='" + name + '\'' +
                ", price='" + price + '\'' +
                ", discount_price='" + discount_price + '\'' +
                ", commission='" + commission + '\'' +
                '}';
    }
}
